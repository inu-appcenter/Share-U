<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.inuappcenter.shareu">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/group326"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@drawable/group326"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">


        <activity android:name=".activity.MainActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".activity.SplashActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"
                    />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.MajorActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".activity.MajorActivity2"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".activity.DetailedNoticeActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".activity.OverallNoticeActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".activity.FileUploadActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".activity.UploadedActivity"
            android:screenOrientation="portrait">

        </activity>
    </application>

</manifest>